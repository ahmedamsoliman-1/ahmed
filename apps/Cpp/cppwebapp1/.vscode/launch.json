{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug cppwebapp1",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/cppwebapp1",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
